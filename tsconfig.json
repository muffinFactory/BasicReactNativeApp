{
  "extends": "@react-native/typescript-config/tsconfig.json",
  "compileOnSave": true,
  "compilerOptions": {
    "target": "es5",
    "baseUrl": ".",
    "strictNullChecks": true,
    // "strict": true,
    "paths": {
      "@assets/*": ["assets/*"],
      "@components/*": ["src/components/*"],
      "@lib/*": ["src/lib/*"],
      "@provider/*": ["src/provider/*"],
      "@services/*": ["src/services/*"],
      "@screens/*": ["src/screens/*"],
      "@storage": ["src/storage"],
      "@ui/*": ["src/ui/*"],
      "@types/*": ["src/types/*"],
      "src/*": ["src/*"]
    },
    "jsx": "react-native",
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "resolveJsonModule": true,
    // "skipLibCheck":true,
    "allowJs": true,
    "outDir": "dist",
    "declaration": true
  },
  "include": ["src"]
}
